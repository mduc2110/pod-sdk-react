{
  "name": "@pod/sdk",
  "version": "1.0.0",
  "type": "module",
  "description": "React library for PodSDK - Embed native iOS/Android WebView containers in React web apps",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
